class Solution {
    public boolean isValid(String s) {
        Stack<Character>st=new Stack<>();
        int i=0;
        while(i<s.length())
        {
            char ch=s.charAt(i);
            if(ch=='('||ch=='{'||ch=='[')
            {
                st.push(ch);
            }else{
                if(st.size()==0)
                {
                    return false;
                }
                char c=st.pop();
                if((c=='('&&ch!=')')||(c=='{'&&ch!='}')||(c=='['&&ch!=']'))
                {
                    return false;
                }
            }
            i++;
        }
        return st.size()==0;
    }
}
